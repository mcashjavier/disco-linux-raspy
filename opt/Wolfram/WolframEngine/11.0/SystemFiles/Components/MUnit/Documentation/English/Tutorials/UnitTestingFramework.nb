   {
    RowBox[{"Test", "[", 
     RowBox[{
      StyleBox["in", "TI"], ",", 
      StyleBox["out", "TI"]}], "]"}], Cell[TextData[{
     "test whether ",
     StyleBox["in", "TI"],
     " evaluates to ",
     StyleBox["out", "TI"],
     " and return a test result object"
    }], "TableText"]},
   {
    RowBox[{"Test", "[", 
     RowBox[{
      StyleBox["in", "TI"], ",", 
      StyleBox["out", "TI"], ",", 
      StyleBox["msg", "TI"]}], "]"}], Cell[
    "specify which messages are expected during evaluation", "TableText"]},
   {
    RowBox[{"TestRun", "[", 
